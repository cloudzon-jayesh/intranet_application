    <script>
    $(document).foundation();

// feeds
    $('.news-feed').click(function() {
        $(this).toggleClass('large-5');
        $(this).toggleClass('large-12');
        $('.activity-feed').toggleClass('left');
        $('.activity-feed').toggleClass('right');
    });

    $('.activity-feed').click(function() {
        $(this).toggleClass('large-5');
        $(this).toggleClass('large-12');
    });


// Slide hover styles 

    $(document).ready(function(){
      $('.slide').mouseover(function(){
          var slide = $(this);
          var type=slide.attr('attrib-type');
            if(type=="txt"){
            slide.attr('src', 'img/txt.1.png')
          //} else {
          //  $(this).attr('src', 'img/doc-hover.png');
          //}
      }
      else if(type=="zip"){
          slide.attr('src', 'img/zip.1.png')
      }   
    });

    $('.slide').mouseout(function(){
        var slide = $(this);
        var type=slide.attr('attrib-type');
        if(type=="txt"){
          slide.attr('src', 'img/txt.png')
        }
        else if(type=="zip"){
          slide.attr('src', 'img/zip.png')
        }
    });
   });

// slide modal 

    $( '.slide-img' ).click(function(){
      $('#myModal').foundation('reveal', 'open'); 
    });

